#include <stdout.h>
#include<stdlib.h>


/**
 * main - GET enviroment variables 
 *
 * Return: Always 0.
 */
int main(int argc, char*argv[], char*envp[])
{
    printf("Access enviroment variables with getenv\n");
    printf("USER=%s\n",getenv("USER"));
    printf("HOME=%s\n",getenv("HOME"));
    printf("UNDEFINED=%s\n",getenv("UNDEFINED"));
   
    return (0);
}
